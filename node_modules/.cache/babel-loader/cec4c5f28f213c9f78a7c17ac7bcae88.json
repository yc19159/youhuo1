: 0,\n      tupian: '',\n      //评论图片文件\n      pic: '',\n      arr: [],\n      valve: false //用户是否提交评论照片的状态,默认没有提交\n\n    };\n  },\n  methods: {\n    //返回上一级目录\n    back: function back() {\n      this.$router.go(-1);\n    },\n    // 整体评价点击\n    clickStars: function clickStars(i) {\n      this.rateScoreText = this.rateScoreDesc[i];\n      this.xing = i + 1;\n      this.level = i + 1;\n\n      if (this.level < 1) {\n        alert(\"请提交星级点评\");\n      }\n\n      console.log(this.level);\n    },\n    //店铺评价点击\n    deliveryStars: function deliveryStars(i) {\n      this.xing2 = i + 1; // console.log(\"点击了送货\"+(i+1)+\"颗星\")\n    },\n    //服务态度点击\n    serviceStars: function serviceStars(i) {\n      this.xing3 = i + 1; // console.log(\"点击了服务\"+(i+1)+\"颗星\")\n    },\n    //点击添加评论\n    addComment: function addComment() {\n      console.log(this.content);\n      this.$axios.post(\"http://192.168.0.22:8080/wx/comment/post\", {\n        type: 0,\n        //类型\n        valueId: 1036002,\n        //商品id\n        content: this.content,\n        //评论内容\n        star: this.level,\n        //评论星级\n        hasPicture: true,\n        //是否上传图片\n        picUrls: this.arr //图片url数组\n\n      }).then(function (res) {\n        console.log(res);\n      });\n    },\n    //添加照片 \n    addImg: function addImg(event) {\n      var _this = this;\n\n      var inputDOM = this.$refs.inputer; //通过DOM获取数据\n\n      this.fil = inputDOM.files;\n      var file = this.$refs.inputer.files[0];\n      console.log(file); //新建表单存储数据\n\n      var data = new FormData();\n      data.append('file', file);\n      this.$axios.post(\"http://192.168.0.22:8080/wx/comment/uploadCommentPic\", data).then(function (res) {\n        //获取到照片追加到新的数组提交\n        _this.pic = res.data.data.picUrl;\n\n        _this.arr.push(_this.pic);\n      });\n      var oldLen = this.imgLen;\n      var len = this.fil.length_oldLen;\n\n      if (len > 2) {\n        alert('最多可以上传3张+您还可以上传' + (3 - oldLen) + '张');\n        return false;\n      }\n\n     